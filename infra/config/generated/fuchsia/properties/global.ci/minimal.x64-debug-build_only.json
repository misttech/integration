{
  "$fuchsia/rbe": {
    "instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "no_python2": true,
  "recipe": "fuchsia/build",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "board_cipd_prefix": "fuchsia/assembly/boards/",
    "board_input_bundle_set_cipd_prefix": "fuchsia/assembly/board_input_bundles/",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/global.ci/minimal.x64-debug-build_only.textproto",
      "incremental": true
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "partitions_cipd_prefix": "fuchsia/assembly/partitions/",
    "platform_artifacts_cipd_path": "fuchsia/assembly/platform/x64"
  }
}
