{
  "recipe": "fuchsia/fuchsia",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/global.ci/bringup.x64-gce.textproto",
      "incremental": true,
      "run_tests": true
    },
    "checkout": {
      "manifest": "flower",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "affected_tests_multiply_threshold": 100,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "enable_sandboxing": true,
      "gce_mediator": {
        "cloud_project": "fuchsia-testing-vms",
        "endpoint": "fuchsia-gce-mediator-prod.uc.r.appspot.com",
        "machine_shape": "n2-standard-4"
      },
      "max_attempts": 1,
      "max_attempts_per_test": 3,
      "per_test_timeout_secs": 300,
      "pool": "fuchsia.tests",
      "swarming_expiration_timeout_secs": 18000,
      "swarming_io_timeout_secs": 360,
      "target_shard_duration_secs": 540,
      "targets_serial": true,
      "test_on_gce": true,
      "upload_results": true,
      "use_cas_for_images": true
    }
  },
  "uses_subbuild": true
}
