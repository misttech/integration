{
  "$fuchsia/rbe": {
    "instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "no_python2": true,
  "recipe": "fuchsia/build",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/global.ci/bringup.x64-gce-asan.textproto",
      "incremental": true,
      "run_tests": true
    },
    "checkout": {
      "enable_submodules": true,
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "botanist_grace_period_secs": 60,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "enable_sandboxing": true,
      "ffx_experiment_level": 1,
      "gce_mediator": {
        "cloud_project": "fuchsia-testing-vms",
        "endpoint": "fuchsia-gce-mediator-prod.uc.r.appspot.com",
        "machine_shape": "n2-standard-4"
      },
      "max_attempts": 1,
      "max_attempts_per_test": 3,
      "pool": "fuchsia.tests",
      "swarming_expiration_timeout_secs": 18000,
      "swarming_grace_period_secs": 30,
      "swarming_io_timeout_secs": 420,
      "targets_serial": true,
      "test_on_gce": true,
      "testsharder_params_path": "integration/infra/config/generated/fuchsia/testsharder_params/global.ci/bringup.x64-gce-asan.textproto",
      "upload_results": true,
      "use_cas_for_images": true
    }
  }
}
