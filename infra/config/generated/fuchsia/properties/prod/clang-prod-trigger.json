{
  "$recipe_engine/swarming": {
    "server": "https://chromium-swarm.appspot.com"
  },
  "bucket": "prod",
  "builders": [
    "clang-linux-x64",
    "clang-mac-x64",
    "clang-windows-x64"
  ],
  "enforce_output": true,
  "nonblocking_builders": [
    "clang-linux-arm64"
  ],
  "recipe": "contrib/toolchain_trigger",
  "timeout": 21600,
  "triggers": [
    "clang-goma",
    "llvm-prod-trigger"
  ]
}