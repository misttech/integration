{
  "$fuchsia/autocorrelator": {
    "ci_bucket": "global.ci"
  },
  "$fuchsia/rbe": {
    "instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "no_python2": true,
  "recipe": "fuchsia/build",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/try/core.x64-release-ffx_subtools.textproto",
      "incremental": true,
      "run_tests": true
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "botanist_grace_period_secs": 60,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "enable_sandboxing": true,
      "ffx_experiment_level": 1,
      "max_attempts_per_test": 3,
      "pool": "fuchsia.tests",
      "skip_unaffected_tests": true,
      "swarming_expiration_timeout_secs": 18000,
      "swarming_grace_period_secs": 30,
      "swarming_io_timeout_secs": 360,
      "testsharder_params_path": "integration/infra/config/generated/fuchsia/testsharder_params/try/core.x64-release-ffx_subtools.textproto",
      "upload_results": true,
      "use_cas": true,
      "use_cas_for_images": true
    }
  }
}
