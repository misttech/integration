{
  "no_python2": true,
  "recipe": "fuchsia/fuchsia",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/try/bringup.arm64-gce-staging.textproto",
      "incremental": true,
      "run_tests": true
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "botanist_grace_period_secs": 60,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "enable_sandboxing": true,
      "gce_mediator": {
        "cloud_project": "bct-staging-fuchsia-vms",
        "endpoint": "fuchsia-gce-mediator-prod.uc.r.appspot.com",
        "machine_shape": "t2a-standard-4"
      },
      "max_attempts_per_test": 3,
      "pool": "fuchsia.tests",
      "skip_unaffected_tests": true,
      "swarming_expiration_timeout_secs": 18000,
      "swarming_grace_period_secs": 30,
      "swarming_io_timeout_secs": 360,
      "targets_serial": true,
      "test_on_gce": true,
      "testsharder_params_path": "integration/infra/config/generated/fuchsia/testsharder_params/try/bringup.arm64-gce-staging.textproto",
      "use_cas_for_images": true
    }
  },
  "uses_subbuild": true
}
