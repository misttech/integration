{
  "no_python2": true,
  "recipe": "fuchsia/fuchsia",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/try/terminal.x64-release-profile.textproto",
      "run_tests": true
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "botanist_grace_period_secs": 60,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "enable_sandboxing": true,
      "max_attempts_per_test": 3,
      "pool": "fuchsia.tests",
      "skip_unaffected_tests": true,
      "swarming_expiration_timeout_secs": 18000,
      "swarming_grace_period_secs": 30,
      "swarming_io_timeout_secs": 660,
      "testsharder_params_path": "integration/infra/config/generated/fuchsia/testsharder_params/try/terminal.x64-release-profile.textproto",
      "upload_results": true,
      "use_cas": true,
      "use_cas_for_images": true,
      "zircon_args": [
        "kernel.x86.disable_spec_mitigations=true"
      ]
    }
  },
  "uses_subbuild": true
}
