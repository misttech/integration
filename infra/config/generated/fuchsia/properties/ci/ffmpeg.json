{
  "$recipe_engine/swarming": {
    "server": "https://chromium-swarm.appspot.com"
  },
  "builds": [
    {
      "fint_params_path": "integration/infra/config/generated/fuchsia/specs/ci/ffmpeg-arm64.fint.textproto",
      "target": "arm64"
    },
    {
      "fint_params_path": "integration/infra/config/generated/fuchsia/specs/ci/ffmpeg-x64.fint.textproto",
      "target": "x64"
    }
  ],
  "enable_property_versioning": true,
  "experimental.rbe_cas": true,
  "manifest": "third_party/ffmpeg",
  "recipe": "contrib/ffmpeg",
  "remote": "https://fuchsia.googlesource.com/integration"
}