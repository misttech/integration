{
  "$fuchsia/rbe": {
    "instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "no_python2": true,
  "recipe": "fuchsia/build",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/ci/clang_toolchain.ci.core.x64-debug-tot-build_only.textproto",
      "report_binary_sizes": true,
      "timeout_secs": 7200
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build"
  }
}
