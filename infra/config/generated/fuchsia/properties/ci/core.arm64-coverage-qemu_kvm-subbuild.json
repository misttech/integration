{
  "$fuchsia/rbe": {
    "instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "no_python2": true,
  "recipe": "fuchsia/build",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/ci/core.arm64-coverage-qemu_kvm.textproto",
      "incremental": true,
      "run_tests": true,
      "timeout_secs": 7200
    },
    "checkout": {
      "attributes": [
        "clang-arm64"
      ],
      "do_not_rebase_patch": true,
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "botanist_grace_period_secs": 60,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "enable_sandboxing": true,
      "ffx_experiment_level": 1,
      "max_attempts": 1,
      "max_attempts_per_test": 3,
      "pool": "fuchsia.tests",
      "swarming_expiration_timeout_secs": 18000,
      "swarming_grace_period_secs": 30,
      "swarming_io_timeout_secs": 660,
      "testsharder_params_path": "integration/infra/config/generated/fuchsia/testsharder_params/ci/core.arm64-coverage-qemu_kvm.textproto",
      "upload_results": true,
      "use_cas": true,
      "use_cas_for_images": true,
      "zircon_args": [
        "kernel.x86.disable_spec_mitigations=true",
        "kernel.port.max-observers=200000"
      ]
    }
  }
}
